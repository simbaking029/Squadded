{
  "name": "squad-widget",
  "version": "0.0.0",
  "description": "",
  "author": "squad-shopping",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern *.dev.js .",
    "precommit": "yarn lint",
    "dev": "env-cmd -f .dev.env nuxt --hostname 0.0.0.0",
    "build": "env-cmd -f .prod.env nuxt build",
    "local:build": "env-cmd -f .dev.env nuxt build",
    "start": "nuxt start",
    "generate": "env-cmd -f .prod.env nuxt generate",
    "test": "yarn lint && env-cmd -f ./test/.env jest --no-cache",
    "test:tap": "yarn test --reporters=jest-tap-reporter --coverage"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.16",
    "nuxt": "^2.8.1",
    "vue": "^2.6.10",
    "vue-i18n": "^8.14.0",
    "vue-native-websocket": "^2.0.13",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@mdi/font": "^4.0.96",
    "@nuxtjs/eslint-config": "^1.1.2",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.2.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "chance": "^1.0.18",
    "env-cmd": "^9.0.3",
    "eslint": "^6.1.0",
    "eslint-config-standard": ">=13.0.1",
    "eslint-plugin-import": ">=2.18.2",
    "eslint-plugin-jest": ">=22.15.1",
    "eslint-plugin-node": ">=9.1.0",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "fetch-mock": "^7.3.9",
    "jest": "^24.8.0",
    "jest-tap-reporter": "^1.9.0",
    "nodemon": "^1.19.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.4",
    "ws": "^7.1.2"
  }
}
