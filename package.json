{
  "name": "squad-widget",
  "version": "0.0.0",
  "description": "",
  "author": "squad-shopping",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --ignore-pattern *.min.js .",
    "precommit": "yarn lint",
    "dev": "env-cmd -f ./dev/.env nuxt --hostname 0.0.0.0",
    "build": "nuxt build",
    "local:build": "env-cmd -f ./dev/.env nuxt build",
    "start": "nuxt start",
    "local:generate": "env-cmd -f ./dev/.env nuxt generate",
    "generate": "nuxt generate",
    "test": "yarn lint && env-cmd -f ./test/.env jest",
    "test:no_lint": "env-cmd -f ./test/.env jest --no-cache",
    "test:find-skipped": "yarn test:no_lint --reporters jest-skipped-reporter",
    "test:tap": "yarn test --reporters=jest-tap-reporter"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/jest.setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2|svg)$": "jest-transform-stub"
    }
  },
  "dependencies": {
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "@sentry/browser": "^5.12.4",
    "@sentry/integrations": "^5.12.4",
    "autosize": "^4.0.2",
    "embedo": "^1.13.0",
    "hammerjs": "^2.0.8",
    "js-base64": "^2.5.2",
    "lodash": "^4.17.15",
    "nuxt": "^2.11.0",
    "vue": "^2.6.11",
    "vue-advanced-cropper": "^0.16.0",
    "vue-animejs": "^2.1.1",
    "vue-i18n": "^8.15.1",
    "vue-image-upload-resize": "^2.3.0",
    "vue-native-websocket": "^2.0.13",
    "vue-observe-visibility": "^0.4.6",
    "vue-responsive-video-background-player": "^1.0.7",
    "vue-template-compiler": "^2.6.11",
    "vue2-touch-events": "^2.2.1",
    "vuex": "^3.1.2",
    "webpack": "^4.41.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.0",
    "@babel/core": "^7.10.0",
    "@babel/plugin-proposal-class-properties": "^7.10.0",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@mdi/font": "^4.7.95",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.1.0",
    "@nuxtjs/vuetify": "^1.9.1",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.0.0",
    "chance": "^1.1.4",
    "css-loader": "^3.4.0",
    "env-cmd": "^10.0.1",
    "eslint": "^6.7.2",
    "eslint-config-standard": ">=14.1.0",
    "eslint-plugin-import": ">=2.19.1",
    "eslint-plugin-jest": ">=23.1.1",
    "eslint-plugin-node": ">=10.0.0",
    "eslint-plugin-nuxt": ">=0.5.0",
    "eslint-plugin-promise": ">=4.2.1",
    "eslint-plugin-standard": ">=4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "fetch-mock": "^8.1.0",
    "jest": "^24.9.0",
    "jest-skipped-reporter": "^0.0.5",
    "jest-tap-reporter": "^1.9.0",
    "jest-transform-stub": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "material-design-icons": "^3.0.1",
    "nodemon": "^2.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.8.3",
    "ws": "^7.2.1"
  }
}
