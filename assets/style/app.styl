
html
	overflow-y unset !important

body
	font-size 16px

	.v-application
		a
			text-decoration none
		.v-icon
			background transparent
		.v-tab
			text-transform capitalize
		.v-btn--icon
			background-color transparent
		.v-list-item
			min-height 26px
		&.normal
			color inherit

button
	outline none

.v-application .theme--light,
#app.theme--light.v-application
	background-color white

.full-width
	width 100%
	margin-left 0
	margin-right 0

.grid
	display grid

.v-responsive__content
	width auto !important

.poll-item,
.scroll-items
	.card_bottom
		padding 8px

.v-btn--icon.v-size--default .v-icon,
.v-btn--fab.v-size--default .v-icon{
    width 24px
    height 5VW
    font-size 5.9vw
}
.theme--light.v-text-field > .v-input__control > .v-input__slot:before {
    display: none;
}
.search-plus i.v-icon.notranslate.sqdi.sqdi-magnifying-glass-finder.theme--light {
    font-size: 3.692vw !important;
}
.search-plus .v-input__prepend-outer {
	margin-left: 2px;
	margin-top: 9px;
}
.choose-items .selected .v-responsive__content{
	box-shadow: none;
	background-image:url('../../assets/img/selected.svg');
	background-size: 21.692vw;
	height: auto;
	background-position: center;
}
.choose-items .selected .v-responsive__content:after{
	opacity: 1;
}
.v-responsive__content:after {
    position: absolute;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.5);
    color: #f1f1f1;
    width: 100%;
    opacity: 0;
    font-size: 20px;
    padding: 20px;
    text-align: center;
    content: '';
    z-index: -1;
}
/*select*/
.row.expire-section .col.col-4 .v-text-field .v-input__slot{
	margin-bottom: 0;
}
.row.expire-section .col.col-4 .v-text-field__details{
	display: none;
}
 .v-text-field.expire-custom-select > .v-input__control > .v-input__slot:after{
	display: none;
}
.v-text-field.expire-custom-select .v-select__slot {
    background: #F4F4F5;
    border-radius: 3.076vw;
	height: 6.4vw;
}
.v-text-field.expire-custom-select .v-input__prepend-inner, .v-text-field.expire-custom-select .v-input__append-inner{
	margin-top: 1px;
}
.v-text-field.expire-custom-select .v-select__selections {
    color: #000;
    font-size: 3.230vw;
    font-weight: 700;
    padding-left: 10px;
}
.v-text-field.expire-custom-select .mdi-menu-down::before {
    content: "";
}
.v-text-field.expire-custom-select .v-input input {
    max-height: 5.846vw;
}
/*select expire*/
.v-menu__content.theme--light.v-menu__content--fixed.menuable__content__active {
    border-radius: 3.076vw;
}
.v-menu__content.theme--light.v-menu__content--fixed.menuable__content__active .v-list {
    padding: 0;
}
.v-menu__content.theme--light.v-menu__content--fixed.menuable__content__active .v-list-item__content {
    padding: 0;
}
.expire-custom-select .v-input__icon{
	background-image:url('../img/custom-select.svg');
	background-size: 1.844vw;
    background-position: center;
	height: 5.6vw;
}
.outfit-card .multi-item .grid.grid-snap-2 {
    grid-template-columns: 1fr 1fr;
    grid-template-rows: 1fr 1fr;
    grid-gap: 10px;
}
.report-dialog.v-dialog
	margin 1px
	border-radius: 9.23vw;
	.v-card
		background rgba(255,255,255,.97)
		.report-options div, .report-options .v-input--selection-controls.v-input .v-label
			background-color transparent
		.report-options 
			padding 0 7.69vw 10px
			.v-messages 
				display none
			.v-input--selection-controls.v-input .v-label
				color #000
				font-weight 600
				font-size 4vw
			.v-input--radio-group--column .v-radio:not(:last-child)
				margin-bottom 3.30vw
			.other-option .v-input__slot
				background-color #F4F4F5
				border-radius 3.07vw
				.v-label.theme--light
					background-color transparent
					font-size 3.23vw
					color #B8B8BA
.filter-tab .v-slide-group__prev.v-slide-group__prev--disabled
	display none !important
.paired-tab .v-tab
	min-width: 85px;
.paired_section
	.outfit-card .multi-item .grid
		grid-gap 0.3vw
	
