
html
	overflow-y unset !important

body
	font-size 16px
	-webkit-overflow-scrolling touch
	.v-application
		a
			text-decoration none
		.v-icon
			background transparent
		.v-tab
			text-transform capitalize
		.v-btn--icon
			background-color transparent
		.v-list-item
			min-height 26px
		&.normal
			color inherit
	&::-webkit-scrollbar-thumb
		background-color #B8B8BA
		outline 0
	&::-webkit-scrollbar
		width 5px
	.v-text-field > .v-input__control > .v-input__slot
		cursor pointer
		input
			cursor pointer
			@media screen and (max-width: 350px)
				padding 3px 0
	input:focus::-webkit-input-placeholder
		color transparent !important
	input:focus::-moz-placeholder
		color transparent !important
	input:-moz-placeholder
		color transparent !important
.signin-process
	.v-text-field > .v-input__control > .v-input__slot
		input
			margin-top 3px
			@media screen and (max-width: 350px)
				padding 3px 0
				margin-top 6px
button
	outline none

/* Chrome, Safari, Edge, Opera */

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button
	-webkit-appearance none
	margin  0

.fancy_scroll
	-webkit-overflow-scrolling touch
	&::-webkit-scrollbar-thumb
		background-color #B8B8BA
		outline 0
	&::-webkit-scrollbar
		width 5px
		height 5px

/* Firefox */
input[type=number]
	-moz-appearance textfield

.v-application .theme--light,
#app.theme--light.v-application
	background-color white

.full-width
	width 100%
	margin-left 0
	margin-right 0

.grid
	display grid

.v-responsive__content
	width auto !important

.poll-item,
.scroll-items
	.card_bottom
		padding 8px

.v-btn--icon.v-size--default .v-icon,
.v-btn--fab.v-size--default .v-icon{
    width 24px
    height 5VW
    font-size 5.9vw
}
.theme--light.v-text-field > .v-input__control > .v-input__slot:before {
    display: none;
}
.search-plus i.v-icon.notranslate.sqdi.sqdi-magnifying-glass-finder.theme--light {
    font-size: 3.692vw !important;
}
.search-plus .v-input__prepend-outer {
	margin-left: 2px;
	margin-top: 9px;
}
.v-responsive__content:after {
    position: absolute;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.5);
    color: #f1f1f1;
    width: 100%;
    opacity: 0;
    font-size: 20px;
    padding: 20px;
    text-align: center;
    content: '';
    z-index: -1;
}
/*select*/
.row.expire-section .col.col-4 .v-text-field .v-input__slot{
	margin-bottom: 0;
}
.row.expire-section .col.col-4 .v-text-field__details{
	display: none;
}
 .v-text-field.expire-custom-select > .v-input__control > .v-input__slot:after{
	display: none;
}
.v-text-field.expire-custom-select .v-select__slot {
    background: #F4F4F5;
    border-radius: 3.076vw;
	height: 6.4vw;
}
.v-text-field.expire-custom-select .v-input__prepend-inner, .v-text-field.expire-custom-select .v-input__append-inner{
	margin-top: 1px;
}
.v-text-field.expire-custom-select .v-select__selections {
    color: #000;
    font-size: 3.230vw;
    font-weight: 700;
    padding-left: 10px;
}
.v-text-field.expire-custom-select .mdi-menu-down::before {
    content: "";
}
.v-text-field.expire-custom-select .v-input input {
    max-height: 5.846vw;
}
/*select expire*/
.v-menu__content.theme--light.v-menu__content--fixed.menuable__content__active {
    border-radius: 3.076vw;
}
.v-menu__content.theme--light.v-menu__content--fixed.menuable__content__active .v-list {
    padding: 0;
}
.v-menu__content.theme--light.v-menu__content--fixed.menuable__content__active .v-list-item__content {
    padding: 0;
}
.expire-custom-select .v-input__icon{
	background-image:url('../img/custom-select.svg');
	background-size: 1.844vw;
    background-position: center;
	height: 5.6vw;
}
.outfit-card .multi-item .grid.grid-snap-2 {
    grid-template-columns: 1fr 1fr;
    grid-template-rows: 1fr 1fr;
    grid-gap: 10px;
}
.outfit-card .multi-item .grid.grid-snap-3 {
    grid-template-columns: 1.68fr 1fr;
    grid-template-rows: 1fr 1fr;
    grid-gap: 10px;
}
.outfit-card .multi-item .grid.grid-snap-4 {
    grid-template-columns 1fr 1fr;
    grid-template-rows: 1fr 1fr;
    grid-gap: 10px;
}
